<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="UI smoke tests">
    <listeners>
        <listener class-name="listeners.RetryTestListener"/>
    </listeners>
    <test name="Smoke customers tests">
        <groups>
            <run>
                <exclude name="broken"/>
            </run>
        </groups>
        <packages>
            <package name="tests"/>
        </packages>
    </test>
</suite>